	<statements>
		<select id="GetServerTime" resultClass="DateTime" parameterClass="string">
			<![CDATA[
     select sysdate from dual
      ]]>
		</select>
    <select id="GetDrpInfoByPID">
      <![CDATA[
            SELECT PTIid As "id"
            ,parenttypeid As "pId"
            ,sign as "sign"
            ,typename AS "name"
            ,typevalue As "value"
            ,typevalue2 as "typevalue2"
            FROM sys_publictypeinfo
            where sign =#sign# 
      ]]>
    </select>
	</statements>
</sqlMap>
