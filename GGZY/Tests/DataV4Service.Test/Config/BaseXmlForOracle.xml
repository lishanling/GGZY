    <typeAlias alias="Sys_User" type="StrongSoftManage.Model.SysManage.Sys_User,StrongSoftManage.Model"/>
    <typeAlias alias="Sys_Area" type="StrongSoftManage.Model.SysManage.Sys_Area,StrongSoftManage.Model"/>
    <typeAlias alias="Sys_Menu" type="StrongSoftManage.Model.SysManage.Sys_Menu,StrongSoftManage.Model"/>
    <typeAlias alias="Sys_UserRole" type="StrongSoftManage.Model.SysManage.Sys_UserRole,StrongSoftManage.Model"/>
